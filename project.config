[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/Raff-97
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/Raff-97
	label-Verified = -1..+1 group user/Raff-97
	push = group user/Raff-97
	read = group user/Raff-97
	rebase = group user/Raff-97
	removeReviewer = group user/Raff-97
	submit = group user/Raff-97
[access "refs/heads/*"]
	create = group user/Raff-97
	push = +force group user/Raff-97
	pushMerge = group user/Raff-97
	read = group user/Raff-97
[access "refs/tags/*"]
	push = group user/Raff-97
	push = +force group user/Raff-97
